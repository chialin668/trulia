
<html>
<head></head>
<body>
	
	<h4>Grade: <%=@grade%>, <%=@test_name%></h4>
	<table cellspacing='0' border='1'>
	<tr>
		<th>Rank</th>
		<th>School Name</th>
		<th>County Name</th>
		<th>District Name</th>
		<th>Adv.</th>
		<th>Prof.</th>
		<th>Total</th>
		<th>TEnroll</th>
		<th>%Tested</th>
	</tr>
	<%rank=0%>
	<% for record in @records %> 
		<tr>
			<%rank=rank+1%>
			<td><%=rank%></td>
			<td><%=record.school_name%> </td>
			<td><%=record.county_name%></td>
			<td><%=record.district_name%></td>
			<td align='right'><%=record.cst_pct_advanced%></td>
			<td align='right'><%=record.cst_pct_proficient%></td>
			<td align='right'><%=record.cst_pct_advanced.to_i+record.cst_pct_proficient.to_i%></td>
			<td align='right'><%=record.start_reported_enrollment%></td>
			<td align='right'><%=record.percent_tested%></td>
		</tr>
	<% end %>	
	</table>
	
</body>
</html>